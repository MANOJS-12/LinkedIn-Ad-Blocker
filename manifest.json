{
    "manifest_version": 2,
    "name": "LinkedIn Ad-Blocker",
    "description": "Blocking the ads in LinkedIn",
    "version": "0.0.1",
    "author": "MANOJ",
    "browser_action": {
        "default_title": "LinkedIn Ad-Blocker"
    },
    "permissions": [
        "tabs",
        "webNavigation",
        "https://www.linkedin.com/"
    ],
    "background": {
        "scripts": [
            "extension.js"
        ]
    }
}